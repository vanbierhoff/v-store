{
  "name": "store-v2",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "build:meta": "node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng build @v/meta-helper",
    "build:store": "node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng build @v/store",
    "build:release": "node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng build @v/meta-helper",
    "build:r": "node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng build @v/r-types",
    "build:e-stack": "node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng build @v/event-stack",
    "test-store:ci": "ng test @v/store --browsers=ChromeHeadlessCI --progress=false --watch=false --code-coverage --source-map=false",
    "test-store": "ng test @v/store --browsers=ChromeHeadlessCI --progress=false --watch=false --code-coverage --source-map=false",
    "test-meta:ci": "ng test @v/meta-helper --browsers=ChromeHeadlessCI --progress=false --watch=false --code-coverage --source-map=false",
    "test-r-type:ci": "ng test @v/r-types --browsers=ChromeHeadlessCI --progress=false --watch=false --code-coverage --source-map=false",
    "test-r-type": "ng test @v/r-types  --code-coverage --source-map=false",
    "publish:store": "cd dist/v/store && npm publish",
    "publish:meta": "cd projects/dist/meta-helper && npm publish",
    "publish:r-types": "cd dist/v/v-types && npm publish",
    "publish:e-stack": "cd dist/v/event-stack && npm publish",
    "typedoc:store": "rm -rf dist/v/store/docs && typedoc --name \\@ex-kernel/forms-core --mode file --out dist/ex-kernel/forms-core/docs --categorizeByGroup true --excludeExternals --excludeNotExported ./projects/ex-kernel/forms-core",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.0.0",
    "@angular/common": "^16.0.0",
    "@angular/compiler": "^16.0.0",
    "@angular/core": "^16.0.0",
    "@angular/forms": "^16.0.0",
    "@angular/platform-browser": "^16.0.0",
    "@angular/platform-browser-dynamic": "^16.0.0",
    "@angular/router": "^16.0.0",
    "@types/validator": "^13.11.1",
    "@v/event-stack": "*",
    "@v/meta-helper": "^0.0.3",
    "@v/r-types": "^0.0.4",
    "lodash": "^4.17.21",
    "reflect-metadata": "^0.1.13",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "typedoc": "^0.16.10",
    "validator": "^13.11.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.0.0",
    "@angular-eslint/builder": "16.0.3",
    "@angular-eslint/eslint-plugin": "16.0.3",
    "@angular-eslint/eslint-plugin-template": "16.0.3",
    "@angular-eslint/schematics": "16.0.3",
    "@angular-eslint/template-parser": "16.0.3",
    "@angular/cli": "~16.0.0",
    "@angular/compiler-cli": "^16.0.0",
    "@types/jasmine": "~4.3.0",
    "@types/lodash": "^4.14.194",
    "@typescript-eslint/eslint-plugin": "5.59.7",
    "@typescript-eslint/parser": "5.59.7",
    "eslint": "^8.40.0",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "karma-junit-reporter": "^2.0.1",
    "ng-packagr": "^16.0.0",
    "typescript": "~5.0.2"
  }
}
